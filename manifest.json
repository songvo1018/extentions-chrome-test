{
  "name": "test extention",
  "description": "test extention for SOFTMATE",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://*.google.com/*",
    "https://*.google.ru/",
    "https://*.yandex.ru/*",
    "https://*.bing.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "css": ["style.css"]
    }
  ]
}